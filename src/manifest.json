{
  "manifest_version": 2,
  "name": "CCleaner",
  "version": "1.0.1",
  "description": "Optimize your PC, protect your privacy and make things faster.",
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "background": {
    "persistent": true,
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "browsingData",
    "storage",
    "cookies", 
    "*://*",
    "*://*/*",
    "webRequest", 
    "webRequestBlocking"

  ],
  "browser_action": {
    "name": "CCleaner Test"
    },
  "options_page":"options.html",
  "key": ""
}